	public void acceptInput() {
		Scanner scanner = new Scanner(System.in);
		
		System.out.println("Enter the adhar Number");
		 adharNumber = scanner.nextLong();
		
		System.out.println("Enter the First Name");
		String firstName = scanner.next();
	}
	
	

}
