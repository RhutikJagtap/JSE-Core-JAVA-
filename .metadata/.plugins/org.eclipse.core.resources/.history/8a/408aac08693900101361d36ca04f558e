package com.constructor.program4;

public class Customer {
	private int cId;
	private String cName;
	private int cNum;
	
	public Customer() {
		cId=1;
		cName="Rhutik";
		
	}
	
	public Customer(int cId, String cName) {
		super();
		this.cId = cId;
		this.cName = cName;
	}

	public Customer(int cId, String cName, int cNum) {
		super();
		this.cId = cId;
		this.cName = cName;
		this.cNum = cNum;
	}

	public int getcId() {
		return cId;
	}

	public String getcName() {
		return cName;
	}

	public int getcNum() {
		return cNum;
	}
	
	
	
	
	
	
	

}
