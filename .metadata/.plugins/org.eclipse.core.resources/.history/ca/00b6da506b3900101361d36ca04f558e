package com.static_variable.program1;

public class Test {
	
	static int a,b;
	
	static {
		System.out.println("Inside Static Block");
		a=10;
		b=20;
	}
	
	static void fun1() {
		System.out.println("Inside Static Method");
	}
	
	int x,y;
	
	{
		System.out.println("Inside Instance Block");
	}
	
	void fun2() {
		System.out.println("Inside Instance method");
	}

}
