package com.constructor.program3;

//constructor overloading
public class CustomerController {
	
	public static void main(String[] args) {
		Customer customer1 = new Customer(1,"tim");
		
		System.out.println(customer1.getcId());
		System.out.println(customer1.getcName());
		System.out.println(customer1.getcNum());
		
		Customer customer2 = new Customer(1,"tim",200);
		
	}

}
