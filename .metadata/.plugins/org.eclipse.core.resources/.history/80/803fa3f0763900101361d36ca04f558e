package com.example;

public class Os {
	
	private String name;
	private int size;
	
	public Os() {
		// TODO Auto-generated constructor stub
	}

	public Os(String name, int size) {
		super();
		this.name = name;
		this.size = size;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getSize() {
		return size;
	}

	public void setSize(int size) {
		this.size = size;
	}
	
	
	
	

}
